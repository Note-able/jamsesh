{
  "name": "jamsesh",
  "version": "0.0.1",
  "description": "An application that helps you meet people and make connections in music.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "test",
    "clean": "rm -rf lib",
    "watch-js": "./node_modules/.bin/babel src  --presets react,stage-1 -d lib",
    "dev-server": "./node_modules/.bin/webpack lib/client/entry.js",
    "test-dev": "node lib/server/webpack",
    "lint": "./node_modules/.bin/eslint ./src",
    "dev": "nodemon lib/server/server",
    "server": "nodemon lib/server/server",
    "start-dev": "npm run build && npm run dev-server && npm run test-dev & npm run server & npm run watch-js",
    "start": "npm run build && npm run dev-server",
    "styles": "./node_modules/.bin/lessc ./public/less/bundle.less ./public/css/bundle.css",
    "build": "npm run clean && ./node_modules/.bin/babel src --presets react,stage-1 -d lib && npm run styles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/note-able/notable.git"
  },
