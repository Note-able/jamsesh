{
  "name": "jamsesh",
  "version": "0.0.1",
  "description": "An application that helps you meet people and make connections in music.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "test",
    "clean": "rm -rf lib",
    "watch-js": "./node_modules/.bin/babel --plugins transform-es2015-classes src --presets react -d lib",
    "dev-server": "./node_modules/.bin/webpack lib/client/entry.js",
    "server": "node lib/server/server",
    "start-dev": "npm run build && npm run dev-server && npm run server",
    "start": "npm run server",
    "build": "npm run clean && ./node_modules/.bin/babel --plugins transform-es2015-classes src --presets react -d lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sportnak/jamsesh.git"
  },
  "author": "Michael Nakayama",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sportnak/jamsesh/issues"
  },
  "homepage": "https://github.com/sportnak/jamsesh#readme",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-syntax-jsx": "^6.3.13",
    "babel-plugin-transform-react-jsx": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "body-parser": "^1.14.2",
    "clean-webpack-plugin": "^0.1.5",
    "express": "^4.13.3",
    "history": "^1.17.0",
    "nodemon": "^1.8.1",
    "path": "^0.12.7",
    "pg": "^4.4.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "react-router": "^1.0.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
