{
  "name": "jamsesh",
  "version": "0.0.1",
  "description": "An application that helps you meet people and make connections in music.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "bundle": "./node_modules/.bin/webpack -p && ./node_modules/.bin/babel ./src/server --out-dir ./lib/src/server && ./node_modules/.bin/babel ./src/util --out-dir ./lib/src/util && ./node_modules/.bin/babel server.js -o ./lib/server.js && ./node_modules/.bin/babel ./src/config -d ./lib/src/config && cp ./src/config/encrypted-production-config ./lib/src/config && cp -a ./views ./lib && cp -a ./public ./lib && cp Noteable-e4d2cea40c15.json ./lib/src && mkdir ./lib/uploads",
    "dist": "npm run icons && rm -rf ./lib && npm run prod-bundle && npm run transpile-server && npm run copy-server",
    "copy-server": "cp ./src/config/encrypted-production-config ./lib/src/config && cp -a ./views ./lib && cp -a ./public ./lib && cp Noteable-e4d2cea40c15.json ./lib/src && mkdir ./lib/uploads",
    "transpile-server": "./node_modules/.bin/babel ./src -d ./lib/src --ignore=./src/shared,./src/client",
    "server-dev": "nodemon --exec ./node_modules/.bin/babel-node src/server/server.js ",
    "prod-bundle": "./node_modules/.bin/webpack -p",
    "icons": "node civilize-svgs.js ./public/icons ./src/shared/components/icons",
    "server": "node lib/src/server/server",
    "mongo": "sudo mongod --dbpath=/data",
    "dev-server": "./node_modules/.bin/webpack && ./node_modules/.bin/webpack-dev-server --config ./webpack.config.js --port 8081 --content-base dist",
    "start-dev": "rm -rf dist && ./node_modules/.bin/webpack && ./node_modules/.bin/webpack-dev-server --config ./webpack.dev.config.js --port 8081 --content-base dist",
    "start": "npm run start-dev",
    "test": "NODE_ENV=local TESTING=true node ./node_modules/.bin/mocha -t 10000 --grep Music"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/note-able/notable.git"
  },
  "author": "Michael Nakayama && Ian Mundy",
  "license": "ISC",
